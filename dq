SELECT 
    SUBSTRING_INDEX(SUBSTRING_INDEX(ITEM_NAME, '_', 2), '_', -1) AS CLAIM,
    SUBSTRING_INDEX(SUBSTRING_INDEX(ITEM_NAME, '_', 3), '_', -1) AS TransID,
    COUNT(*) AS TransCount
FROM your_table
GROUP BY CLAIM, TransID;


SELECT 
    SUBSTRING_INDEX(SUBSTRING_INDEX(ITEM_NAME, '_', 2), '_', -1) AS CLAIM,
    COUNT(*) AS TransCount
FROM your_table
GROUP BY CLAIM;
